{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "rules": { 
    "no-console": false,
    "only-arrow-functions": false,
    "no-debugger": false,
    "no-unused-variable": false,
    "no-shadowed-variable": false,
    "no-bitwise": false,
    "interface-over-type-literal": false,
    "jsx-no-lambda": false
  }, 
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  }
}
